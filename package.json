{
  "name": "styling-adventures-admin",
  "version": "1.0.0",
  "description": "Welcome to the content management dashboard for *Styling Adventures with Lala*. This tool powers all uploads, category management, and dashboard control for episodes, voiceovers, and closet items.",
  "main": "server.js",
  "type": "commonjs",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon --watch server.js --ext js,json --exec node server.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "fullstack": "concurrently \"npm run server\" \"npm run dev\"",
    "deploy:rules": "firebase deploy --only firestore:rules,storage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/angelcreator113/styling-adventures-admin.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/angelcreator113/styling-adventures-admin/issues"
  },
  "homepage": "https://github.com/angelcreator113/styling-adventures-admin#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "firebase": "^12.0.0",
    "firebase-admin": "^13.4.0",
    "multer": "^2.0.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.7.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.10",
    "vite": "^7.0.6"
  }
}
